#!/bin/sh

checkExitCode() {
if [ $? -ne 0 ]; then
	echo "Error"
	exit 1;
fi
}

BASEDIR=`dirname $0`

if [ $# -lt 1 ]; then echo "Error No inputs provided" && exit 1; fi

. $BASEDIR/io.sh 2 6 1 "$@"

echo $R_HOME/bin/Rscript --no-save --no-restore $BASEDIR/mergeSNVAndMAF.R $INPUTS1 $INPUTS2 $PARAMS1 $PARAMS2 $PARAMS3 $PARAMS4 $PARAMS5 $PARAMS6 $OUTPUTS1
$R_HOME/bin/Rscript --no-save --no-restore $BASEDIR/mergeSNVAndMAF.R $INPUTS1 $INPUTS2 $PARAMS1 $PARAMS2 $PARAMS3 $PARAMS4 $PARAMS5 $PARAMS6 $OUTPUTS1

checkExitCode
